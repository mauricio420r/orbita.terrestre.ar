{
  "version": 2,
  "headers": [
    {
      "source": "/api/proxy(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=28800"
        }
      ]
    },
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
